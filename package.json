{
  "name": "echots",
  "version": "0.1.0",
  "description": "A TypeScript framework focused on modularity and strong data management, with advanced handling of models and collections for building scalable, data-driven single-page applications.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CrackinLLC/EchoTS.git"
  },
  "keywords": [
    "ui",
    "framework",
    "typescript",
    "modular",
    "data management"
  ],
  "author": "Crackin Studios <contact@crackin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CrackinLLC/EchoTS/issues"
  },
  "homepage": "https://github.com/CrackinLLC/EchoTS#readme",
  "dependencies": {
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "typescript": "^4.0.0"
  }
}
